{
  "name": "x6",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "src/**/*.ts"
  ],
  "directories": {
    "lib": "lib/",
    "src": "src/"
  },
  "keywords": [
    "graph",
    "editor"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "test": "jest",
    "dev": "webpack-dev-server --open --config webpack.config.js",
    "less": "lessc ./src/index.less ./lib/index.css",
    "build:images": "node ./scripts/make-images.js",
    "build:version": "node ./scripts/make-version.js",
    "build": "yarn run build:images && yarn run build:version && yarn run less && tsc",
    "lint": "tslint -c tslint.json -p tsconfig.lint.json --fix",
    "coveralls": "cat ./test/coverage/lcov.info | coveralls",
    "prepublish": "yarn test",
    "prebuild": "yarn run lint && yarn run clean",
    "pretest": "rimraf ./test/coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tslint -c tslint.json -p ./tsconfig.json --fix"
    ]
  },
  "author": {
    "name": "bubkooo",
    "email": "bubkoo.wy@gmail.com"
  },
  "contributor": [],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "@types/mousetrap": "^1.6.3",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.5",
    "@types/webpack-env": "^1.14.0",
    "base64-img": "^1.0.4",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.3",
    "image-size": "^0.7.4",
    "jest": "^24.8.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.1",
    "normalize.css": "^8.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "rimraf": "^2.6.3",
    "simulate-event": "^1.4.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "dependencies": {
    "mousetrap": "^1.6.3"
  }
}
