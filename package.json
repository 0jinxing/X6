{
  "name": "x6",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "src/**/*.ts"
  ],
  "directories": {
    "lib": "lib/",
    "src": "src/"
  },
  "keywords": [
    "graph",
    "editor"
  ],
  "scripts": {
    "clean": "rimraf ./lib",
    "test": "jest",
    "build:images": "node ./scripts/make-images.js",
    "build:version": "node ./scripts/make-version.js",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.lint.json --fix",
    "coveralls": "cat ./test/coverage/lcov.info | coveralls",
    "prepublish": "yarn test",
    "prebuild": "yarn run lint && yarn run clean && yarn run build:images && yarn run build:version",
    "pretest": "rimraf ./test/coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "src/**/*.ts": [
        "tslint -c tslint.json -p ./tsconfig.json --fix"
      ]
    }
  },
  "author": {
    "name": "bubkooo",
    "email": "bubkoo.wy@gmail.com"
  },
  "contributor": [],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.17",
    "base64-img": "^1.0.4",
    "coveralls": "^3.0.6",
    "husky": "^3.0.3",
    "image-size": "^0.7.4",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.5.3"
  }
}
